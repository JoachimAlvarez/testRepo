quote location: :keep do      
  # some dependencies
  import DevOpsCore.SlaveModule
  
  # in celcius
  tempMAX = 10
  tempMIN = 9
  
  # 1/0

  Enum.each Enum.to_list(0..999999), fn x ->
    resultTemp = :rand.uniform((tempMAX - tempMIN) * 10)
    resultTemp = (resultTemp / 10) + tempMIN
    
    # resultTemp = resultTemp + 273.15 # convert to Kelvin
    resultTemp = (resultTemp * 1.8) + 32  # convert to Fahrenheit
    
    return resultTemp
    
    :timer.sleep 1000
  end 
end
